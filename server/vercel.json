{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/old-path",
      "status": 301,
      "headers": {
        "Location": "/new-path"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "API_KEY": "@api_key"
  }
}